<div class="container">
    <form id="update-student" data-id={{student.id}}>
        <div class="form-group">
            <label for="name">Name:</label>
            <input type="text" class="form-control" id="name-update" value={{student.name}}>
        </div>
        <div class="form-group">
            <label for="GPA">GPA:</label>
            <input type="text" class="form-control" id="GPA-update" value={{student.GPA}}>
        </div>
        <button type="submit" class="btn btn-default">Update</button>
    </form>
</div>